<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="algol.diasvividos.Nascimento_input"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:text="@string/segundosTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        android:layout_marginLeft="16dp"
        ads:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="220dp"
        ads:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginBottom="8dp"
        ads:layout_constraintBottom_toTopOf="@+id/adView2"
        ads:layout_constraintVertical_bias="0.1"
        ads:layout_constraintRight_toLeftOf="@+id/Segundos"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:text="@string/minutosTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/textView20"
        android:layout_marginLeft="16dp"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toLeftOf="@+id/Minutos"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="@string/horario"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.838"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/hora" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="serif"
        android:text="@string/dia"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintTop_toBottomOf="@+id/data" />

    <EditText
        android:id="@+id/hora"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="none"
        android:text="@string/horaInputada"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/data"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/data"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/data"
        android:layout_width="150dp"
        android:layout_height="42dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="none"
        android:text="@string/diaInputado"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="206dp"
        android:layout_height="42dp"
        android:clickable="false"
        android:text="@string/nascimento"
        android:textAlignment="center"
        android:textSize="36sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/data"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        ads:layout_constraintVertical_bias="0.181"
        ads:layout_constraintBottom_toTopOf="@+id/data"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="30dp" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id"
        tools:layout_constraintBottom_creator="1"
        ads:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="1dp" />

    <TextView
        android:id="@+id/Minutos"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultadoMinuto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/Segundos"
        android:layout_marginRight="8dp"
        ads:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/Segundos"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultadoSegundo"
        android:textAlignment="center"
        android:textSize="30sp"
        android:layout_marginRight="16dp"
        ads:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="220dp"
        ads:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginBottom="8dp"
        ads:layout_constraintBottom_toTopOf="@+id/adView2"
        ads:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/dias"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultado"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/horas"
        tools:layout_editor_absoluteX="177dp"
        android:layout_marginRight="8dp"
        ads:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/Semanas"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultadoSemana"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/dias"
        android:layout_marginRight="8dp"
        ads:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/Meses"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultadoMes"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/Semanas"
        android:layout_marginRight="16dp"
        ads:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        ads:layout_constraintTop_toBottomOf="@+id/textView"
        ads:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/horas"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:text="@string/resultadoHora"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/Minutos"
        android:layout_marginRight="8dp"
        ads:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="11dp"
        android:layout_marginStart="16dp"
        android:text="@string/semanasTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/textView16"
        android:layout_marginLeft="16dp"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toLeftOf="@+id/Semanas"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:text="@string/diasTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/textView17"
        android:layout_marginLeft="16dp"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toLeftOf="@+id/dias"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:text="@string/horasTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/textView19"
        android:layout_marginLeft="16dp"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toLeftOf="@+id/horas"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:text="@string/mesesTexto"
        android:textAlignment="center"
        android:textSize="30sp"
        ads:layout_constraintBottom_toTopOf="@+id/textView5"
        ads:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginLeft="16dp"
        ads:layout_constraintVertical_bias="1.0"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toLeftOf="@+id/Meses"
        android:layout_marginRight="8dp" />

</android.support.constraint.ConstraintLayout>
